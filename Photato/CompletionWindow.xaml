<Window x:Class="Photato.CompletionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Photato"
        mc:Ignorable="d"
        Title="CompletionWindow" Padding="0" Height="200" Width="350" WindowStyle="None" ResizeMode="NoResize" WindowStartupLocation="CenterOwner">
    <Grid>
        <Grid Margin="0,0,0,164" Background="White">
            <Border BorderBrush="#cccedb" BorderThickness="0,0,0,1"/>
            <Button x:Name="CloseButton" Style="{StaticResource CloseButton}" Content="&#xE106;" Click="CloseButton_Click" Margin="307,3,3,3"/>
            <Label Content="PHOTATO IS DELICIOUS" Margin="0,0,208,0" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Height="36" Width="142"/>
        </Grid>
        <Label x:Name="commentLabel" Content="변환을 완료 했습니다." Margin="20,56,20,110" FontSize="18" />
        <Button x:Name="completeButton" Style="{StaticResource DefaultButton}" Content="OK" Click="completeButton_Click" Margin="200,136,20,20" FontSize="14"/>
        <Border BorderBrush="#cccedb" BorderThickness="1"/>
    </Grid>
</Window>
